{
  "app": {
    "bundleName": "com.drafire.dictionary",
    "vendor": "drafire",
    "version": {
      "code": 1,
      "name": "1.0"
    },
    "apiVersion": {
      "compatible": 3,
      "target": 3
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.drafire.dictionary",
    "name": ".dictionary",
    "reqCapabilities": [
      "video_support"
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.INTERNET",
        "reason": "internet",
        "usedScene":{
          "ability": ["com.drafire.dictionary.MainAbility"],
          "when": "always"
        }
      }
    ],
    "deviceType": [
      "tv",
      "wearable"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "landscape",
        "formEnabled": false,
        "name": "com.drafire.dictionary.MainAbility",
        "icon": "$media:dictionary",
        "description": "$string:mainability_description",
        "label": "电子词典",
        "type": "page",
        "launchType": "standard"
      }
    ]
  }
}